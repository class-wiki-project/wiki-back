<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.wings.mywiki.dao.mybatis.mapper.OnlineMapper">
  <cache />
  

  
    <resultMap id="resultMap_online" type="com.wings.mywiki.model.OnlinVO">
    <result property="onlineId" column="online_id" />
	<result property="keyId" column="key_id" />
	<result property="userId" column="user_id" />
</resultMap>
  
  <select id="select" resultMap="resultMap_online">
  	select * from 
  	online where key_id=#{keyId}
  </select>
 <select id="Login" resultType="int">
		SELECT COUNT(*)
		FROM online
		WHERE
		key_id=#{key_id}
	</select>
	
  
  <insert id="insert">
		INSERT INTO online (key_id, user_id) values (#{keyId}, #{userId})
	</insert>
	<delete id="delete">
		delete from online where key_id=#{key_id}
	</delete>
</mapper>